    <string name="magic_answer_17">De ce que je peux voir, oui.</string>
    <string name="magic_answer_18">C\'est certain.</string>
    <string name="magic_answer_19">C\'est comme ça.</string>
    <string name="magic_answer_20">J\'ai de belles visions.</string>
