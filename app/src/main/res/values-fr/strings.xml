    <string name="hint_magic_ball">Appuyez sur le bouton ci-dessous</string>
    <string name="magic_answer_1">Non.</string>
    <string name="magic_answer_2">Oui!</string>
    <string name="magic_answer_3">Je ne pense pas…</string>
    <string name="magic_answer_4">Peut-être!</string>
    <string name="magic_answer_5">Ca ne fait aucun sens !</string>
    <string name="magic_answer_6">Probablement…</string>
