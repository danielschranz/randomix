    <string name="magic_answer_22">Sans un doute.</string>
    <string name="magic_answer_23">Les réponses sont floues, reéssaye.</string>
    <string name="magic_answer_24">Il vaudrait mieux ne pas en parler.</string>
    <string name="magic_answer_25">Je ne peux pas prédire cela !</string>
    <string name="magic_answer_26">Concentre-toi et repose la question.</string>
    <string name="magic_answer_27">Ne compte pas la dessus !</string>
    <string name="magic_answer_28">Mes sources m\'informent que non.</string>
    <string name="magic_answer_29">Très douteux.</string>
    <string name="magic_answer_30">Demande à un ami.</string>
