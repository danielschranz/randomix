    <string name="delete_all_roulette_confirmation">¿Está seguro de que desea eliminar todas las opciones ingresadas?</string>
    <!-- Coin -->
    <string name="hello_coin_fragment">Modo moneda</string>
    <string name="description_coin">Lanzar una moneda es fácil, pero puede ayudarte a decidir cuando haces algo o no. Solo cara o cruz. ¡Deja que la moneda hable!</string>
    <string name="flip_coin">¡Lanza la moneda!</string>
    <string name="hint_coin">¡Presione el botón de abajo!</string>
    <string name="result_head">¡Cara!</string>
    <string name="result_tail">¡Cruz!</string>
    <!-- Magic Ball -->
    <string name="hello_magic_ball_fragment">Bola Mágica</string>
    <string name="description_magic_ball">¡La bola mágica te dirá las respuestas! Simplemente pregunte algo y lo ayudará con una respuesta genérica y asequible. ¡Puedes activar su grosería desde la configuración!</string>
    <string name="shake_ball">¡Sacudela!</string>
    <string name="hint_magic_ball">¡Presione el botón de abajo!</string>
    <string name="magic_answer_1">No.</string>
    <string name="magic_answer_2">¡Si!</string>
    <string name="magic_answer_3">No lo creo.</string>
    <string name="magic_answer_4">¡Quizás!</string>
    <string name="magic_answer_5">Esto no tiene sentido.</string>
    <string name="magic_answer_6">Probablemente…</string>
    <string name="magic_answer_7">Estas seguro/a?</string>
    <string name="magic_answer_8">Probablemente si.</string>
    <string name="magic_answer_9">Probablemente no.</string>
    <string name="magic_answer_10">Por supuesto.</string>
    <string name="magic_answer_11">Pregúntalo tú mismo.</string>
    <string name="magic_answer_12">No, absurdo.</string>
    <string name="magic_answer_13">¡Diablos si!</string>
    <string name="magic_answer_14">Piénsalo.</string>
    <string name="magic_answer_15">Quién sabe.</string>
    <string name="magic_answer_16">¿Estás bromeando?</string>
    <string name="magic_answer_17">Como yo lo veo, sí.</string>
    <string name="magic_answer_18">Es cierto.</string>
    <string name="magic_answer_19">Es decididamente así.</string>
    <string name="magic_answer_20">Los pronósticos son buenos.</string>
    <string name="magic_answer_21">Las señales apuntan a que sí.</string>
    <string name="magic_answer_22">Sin duda.</string>
    <string name="magic_answer_23">Respuesta confusa, intenta otra vez.</string>
    <string name="magic_answer_24">Mejor no decirte ahora.</string>
    <string name="magic_answer_25">¡No se puede predecir ahora!</string>
    <string name="magic_answer_26">Concéntrate y pregunta otra vez.</string>
    <string name="magic_answer_27">No cuentes con eso.</string>
    <string name="magic_answer_28">Mis fuentes dicen que no.</string>
    <string name="magic_answer_29">Muy dudoso.</string>
    <string name="magic_answer_30">Pregunta a tu mejor amigo/a.</string>
    <string name="magic_answer_rude_1">No, idiota.</string>
    <string name="magic_answer_rude_2">Por supuesto, maldito.</string>
    <string name="magic_answer_rude_3">¿Eres estúpido/a?</string>
    <string name="magic_answer_rude_4">¡Jajaja! Que estúpido.</string>
    <string name="magic_answer_rude_5">Estas loco/a.</string>
    <string name="magic_answer_rude_6">Vete al diablo.</string>
