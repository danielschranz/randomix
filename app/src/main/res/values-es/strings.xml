    <!-- Roulette -->
    <string name="hello_roulette_fragment">Modo ruleta</string>
    <string name="description_roulette">¿Tienes que elegir algo? ¿Estás cansado de las encuestas y amigos mandones? ¡Una ruleta decidirá por ti! Mantenga presionada la ruleta para agregar opciones genéricas o borrarlas.</string>
    <string name="spin_roulette">¡Gira la ruleta!</string>
    <string name="insert_roulette_hint">Ingrese una opción…</string>
    <string name="delete_roulette_hint">Eliminar las últimas opciones ingresadas</string>
    <string name="insert_roulette_description">Enumere a continuación las posibles opciones para agregar en la ruleta.</string>
    <string name="no_entry_roulette">¡Por favor, ingrese 2 o más opciones para girar la ruleta!</string>
    <string name="too_much_entries_roulette">¡No puedes ingresar más de 30 opciones!</string>
    <string name="generic_option">Opción</string>
    <string name="recent_title">Opciones recientes, toque para restaurar</string>
    <string name="bottom_sheet_no_option">Sin opción reciente, ¡gire la ruleta para guardar las opciones actuales!</string>
