    <!-- Magic Ball -->
    <string name="hello_magic_ball_fragment">Toverbal</string>
    <string name="description_magic_ball">De toverbal zal je de antwoorden vertellen! Vraag gewoon iets en het helpt je met een generiek en betaalbaar antwoord. Je kunt de onbeschoftheid via de instellingen activeren!</string>
    <string name="shake_ball">Schud het!</string>
    <string name="hint_magic_ball">Druk op de knop hieronder!</string>
    <string name="magic_answer_1">Nee.</string>
    <string name="magic_answer_2">Ja!</string>
    <string name="magic_answer_3">Ik denk het niet.</string>
    <string name="magic_answer_4">Misschien!</string>
    <string name="magic_answer_5">Dat slaat nergens op.</string>
    <string name="magic_answer_6">Waarschijnlijk…</string>
    <string name="magic_answer_7">Weet je het zeker?</string>
