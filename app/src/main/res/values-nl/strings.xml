    <string name="magic_answer_25">Ik kan het nu niet voorspellen!</string>
    <string name="magic_answer_26">Concentreer en vraag opnieuw.</string>
    <string name="magic_answer_27">Reken er maar niet op.</string>
    <string name="magic_answer_28">Mijn bronnen zeggen van niet.</string>
    <string name="magic_answer_29">Zeer twijfelachtig.</string>
    <string name="magic_answer_30">Vraag je beste vriend.</string>
